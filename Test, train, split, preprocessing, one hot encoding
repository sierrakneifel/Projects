        "def find_greater_200_k(input):\n",
        "  if input > 200000:\n",
        "    return 1.0\n",
        "  else:\n",
        "    return 0.0"
      ],
      "metadata": {
        "id": "GZ06SL8c2Vve"
      },
      "execution_count": null,
      "outputs": []
    },
    {
